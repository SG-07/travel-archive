<% layout('/layout/boilerplate') %>
<div class="row mt-3">
    <h2 class="col-6 offset-3 mb-2">Singup For Travel Archive</h2>
    <div class="col-6 offset-3">
        <form method="POST" action="/login" class="needs-validation" novalidate>
            <div class="mb-3 mt-2">
                <label for="username" class="form-label">Username</label>
                <input name="username" id="username" class="form-control" placeholder="Enter your username" type="text" required>
                <div class="valid-feedback">Looks Good</div>
                <div class="invalid-feedback">Provide Username</div>
            </div>
            <div class="mb-3 mt-2">
                <label for="password" class="form-label">Password</label>
                <input name="password" id="password" class="form-control" placeholder="Enter your password" type="password" required>
                <div class="invalid-feedback">Provide password</div>
            </div>
            <button class="btn btn-outline-primary">Singup</button>
        </form>
    </div>
</div>